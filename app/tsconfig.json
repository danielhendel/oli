{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
      "rootDir": ".",
      "outDir": "../dist-types/app",
      "tsBuildInfoFile": "../node_modules/.cache/tsbuildinfo/app.tsbuildinfo",
  
      "baseUrl": "..",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "references": [{ "path": "../lib" }],
    "include": ["./**/*", "../expo-env.d.ts"],
    "exclude": [
      "**/*.test.*",
      "**/__tests__/**",
      "../node_modules",
      "../dist",
      "../dist-types",
      "../build",
      "../coverage"
    ]
  }
  
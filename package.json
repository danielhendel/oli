    "emulators": "firebase emulators:start --only firestore,auth",
    "seed": "node scripts/seed-firestore.mjs",
    "dev:emu": "npm run env:use:dev && expo start",
    "prod": "npm run env:use:prod && expo start",
    "prestart": "node scripts/check-env.mjs",
    "preios": "node scripts/check-env.mjs",
    "preandroid": "node scripts/check-env.mjs"

{
  "name": "oli",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "fb:login": "firebase login",
    "fb:use": "firebase use oli-backend",
    "fb:emulators": "firebase emulators:start",
    "fb:deploy:functions": "firebase deploy --only functions:createUserProfile",
    "fb:functions:list": "firebase functions:list",
    "fb:functions:logs": "firebase functions:log --only createUserProfile --limit 50"
  },
  "dependencies": {
    "expo": "~53.0.22",
    "expo-router": "~5.1.5",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.2.1",
    "jest-expo": "~53.0.10",
    "react": "19.0.0",
    "react-native": "0.79.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.12",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "firebase-tools": "^14.19.1",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "jest": {
    "preset": "jest-expo"
  }
}

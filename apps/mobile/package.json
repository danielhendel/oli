{
  "name": "@oli/mobile",
  "private": true,
  "version": "0.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "node ./scripts/prestart.cjs && expo start",
    "start:clean": "node ./scripts/prestart.cjs && expo start -c",
    "start:guarded": "PORT=8082 node ./scripts/expo-start.cjs",
    "ios": "node ./scripts/prestart.cjs && expo run:ios",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.cjs --max-warnings=0",
    "test": "jest --config jest.config.js",
    "test:ci": "jest --ci --config jest.config.js",
    "test:rules": "cross-env RUN_RULES_TESTS=1 npx firebase emulators:exec --only firestore \"npm test -- __tests__/firestore.rules.test.ts\"",
    "android": "expo run:android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "expo": "^54.0.13",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.16",
    "expo-linking": "8.0.8",
    "expo-random": "^14.0.1",
    "expo-router": "~6.0.12",
    "expo-web-browser": "~15.0.8",
    "firebase": "^10.12.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@expo/cli": "0.19.4",
    "@firebase/rules-unit-testing": "^3.0.4",
    "@react-native-community/cli": "^10.2.7",
    "@react-native-community/cli-server-api": "^10.1.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.2",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.36.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "packageManager": "npm@10",
  "overrides": {}
}

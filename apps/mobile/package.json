{
  "name": "@oli/mobile",
  "private": true,
  "version": "0.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "node ./scripts/prestart.cjs && expo start",
    "start:clean": "node ./scripts/prestart.cjs && expo start -c",
    "start:guarded": "PORT=8082 node ./scripts/expo-start.cjs",
    "ios": "node ./scripts/prestart.cjs && expo run:ios",
    "android": "expo run:android",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.cjs --max-warnings=0",
    "test": "jest --config jest.config.js",
    "test:ci": "jest --ci --config jest.config.js",
    "test:rules": "cross-env RUN_RULES_TESTS=1 npx firebase emulators:exec --only firestore \"npm test -- __tests__/firestore.rules.test.ts\"",
    "test:node": "jest --config jest.node.config.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "7.1.12",
    "@react-navigation/native-stack": "7.3.16",
    "expo": "54.0.23",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.16",
    "expo-linking": "8.0.8",
    "expo-router": "6.0.12",
    "expo-splash-screen": "~31.0.10",
    "expo-web-browser": "~15.0.8",
    "firebase": "10.12.4",
    "react": "18.2.0",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@expo/cli": "0.19.4",
    "@firebase/rules-unit-testing": "3.0.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.2",
    "@types/react": "18.2.46",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.36.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|expo|@expo|expo-router)"
    ]
  },
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "packageManager": "npm@10"
}

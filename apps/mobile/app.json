{
  "expo": {
    "name": "olimobile",
    "slug": "oli-mobile",
    "scheme": "oli",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.olifitness.olimobile",
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.olifitness.olimobile"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1",
            "cocoapodsVersion": "1.15.2"
          }
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "eas": {
        "projectId": "dd9ba89c-b478-4349-9672-18f7b02e2105"
      }
    }
  }
}
